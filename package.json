  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:testing": "yarn build --mode testing",
    "preview": "vite preview",
    "serve": "serve dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "hardhat:dev": "concurrently \"yarn hardhat:chain\" \"yarn hardhat:fund\"",
    "hardhat:chain": "hardhat node",
    "hardhat:fund": "HARDHAT_NETWORK=localhost node hardhat/fund.js",
    "test": "env-cmd -f .env yarn synpress run --config supportFile='tests/e2e/support.js'",
    "//test": "env-cmd -f .env yarn synpress run -cf synpress.json --config supportFile='support.js'",
    "test:watch": "env-cmd -f .env npx synpress open -cf synpress.json"
